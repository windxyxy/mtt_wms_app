<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/relativeLayout">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="上架完成"
                android:id="@+id/textView12"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textSize="25dp"
                android:textIsSelectable="false" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:id="@+id/imageView6"
                android:layout_alignParentBottom="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:background="#e6e6e6"
                android:layout_centerVertical="true" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="返回"
                android:id="@+id/button26"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:singleLine="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/relativeLayout13">

            <include
                android:id="@+id/relativeLayout_main"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                layout="@layout/bottom_state"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/relativeLayout"
            android:layout_above="@+id/relativeLayout13">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="采购单："
                android:id="@+id/textView52"
                android:layout_marginLeft="98dp"
                android:layout_marginStart="98dp"
                android:layout_marginTop="35dp"
                android:textSize="16dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="货主："
                android:id="@+id/textView56"
                android:textSize="16dp"
                android:layout_below="@+id/textView52"
                android:layout_toLeftOf="@+id/textView62"
                android:layout_toStartOf="@+id/textView62"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="上架单号："
                android:id="@+id/textView58"
                android:textSize="16dp"
                android:layout_below="@+id/textView56"
                android:layout_alignRight="@+id/textView56"
                android:layout_alignEnd="@+id/textView56"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="订单数："
                android:id="@+id/textView59"
                android:textSize="16dp"
                android:layout_below="@+id/textView58"
                android:layout_alignRight="@+id/textView56"
                android:layout_alignEnd="@+id/textView56"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="剩下货位："
                android:id="@+id/textView60"
                android:textSize="16dp"
                android:layout_below="@+id/textView59"
                android:layout_alignLeft="@+id/textView59"
                android:layout_alignStart="@+id/textView59"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="剩下货品："
                android:id="@+id/textView61"
                android:textSize="16dp"
                android:layout_below="@+id/textView66"
                android:layout_toLeftOf="@+id/textView66"
                android:layout_toStartOf="@+id/textView66"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="CG10012124"
                android:id="@+id/textView62"
                android:layout_alignTop="@+id/textView52"
                android:layout_toRightOf="@+id/textView52"
                android:layout_toEndOf="@+id/textView52"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text=""
                android:id="@+id/textView63"
                android:layout_alignTop="@+id/textView56"
                android:layout_alignLeft="@+id/textView62"
                android:layout_alignStart="@+id/textView62"
                android:textIsSelectable="false"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="SJD10012124"
                android:id="@+id/textView64"
                android:layout_alignTop="@+id/textView58"
                android:layout_toRightOf="@+id/textView58"
                android:layout_toEndOf="@+id/textView58"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:id="@+id/textView65"
                android:text="1"
                android:layout_alignTop="@+id/textView59"
                android:layout_toRightOf="@+id/textView59"
                android:layout_toEndOf="@+id/textView59"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:id="@+id/textView66"
                android:layout_alignTop="@+id/textView60"
                android:layout_toRightOf="@+id/textView60"
                android:layout_toEndOf="@+id/textView60"
                android:textSize="16dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="0"
                android:id="@+id/textView67"
                android:layout_alignBottom="@+id/textView61"
                android:layout_alignLeft="@+id/textView66"
                android:layout_alignStart="@+id/textView66"
                android:textSize="16dp" />
        </RelativeLayout>
    </RelativeLayout>
</LinearLayout>